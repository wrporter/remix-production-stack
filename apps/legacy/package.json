{
  "name": "remix-production-stack-d828",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "engines": {
    "node": "22"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:remix": "remix vite:build --sourcemapClient --sourcemapServer",
    "build:server": "tsx scripts/build-server.ts",
    "dev": "tsx watch --clear-screen=false --ignore \".cache/**\" --ignore \"app/**\" --ignore \"vite.config.ts.timestamp-*\" --ignore \"build/**\" --ignore \"node_modules/**\" ./server/main.ts",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "start": "cross-env NODE_ENV=production node --env-file=.env server-build/main.js",
    "start:mocks": "cross-env NODE_ENV=production MOCKS=true tsx .",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "typecheck": "tsc"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/server-build"
  ],
  "dependencies": {
    "@nextui-org/react": "^2.3.6",
